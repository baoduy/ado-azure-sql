{
  "manifestVersion": 1,
  "id": "ado-azure-tools",
  "version": "0.1.16",
  "name": "Azure DevOps Azure Tools.",
  "description": "The helpful Azure Tools for Azure DevOps.",
  "publisher": "Duy",
  "public": true,
  "icons": {
    "default": "static/images/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": ["Azure Pipelines"],
  "categories": ["Azure Pipelines"],
  "content": {
    "license": {
      "path": "LICENSE"
    },
    "details": {
      "path": "README.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/baoduy/ado-azure-tools"
  },
  "files": [
    { "path": "azure-log" },
    { "path": "azure-sql" },
    { "path": "azure-streamjob-control" }
  ],
  "contributions": [
    {
      "id": "ado-azure-log",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "azure-log"
      }
    },
    {
      "id": "ado-azure-sql",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "azure-sql"
      }
    },
    {
      "id": "ado-azure-streamjob-control",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "azure-streamjob-control"
      }
    }
  ]
}
