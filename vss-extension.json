{
  "manifestVersion": 1,
  "id": "ado-azure-tools",
  "version": "1.0.7",
  "name": "Azure Devops Azure Tools.",
  "description": "The helpfull Azure Tools for Azure Devops.",
  "publisher": "Duy",
  "public": false,
  "icons": {
    "default": "static/images/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": ["Azure Pipelines"],
  "categories": ["Azure Pipelines"],
  "content": {
    "license": {
      "path": "LICENSE"
    },
    "details": {
      "path": "README.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/baoduy/ado-azure-tools"
  },
  "files": [
    {
      "path": "tools"
    }
  ],
  "contributions": [
    {
      "id": "ado-azure-tools",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "ado-azure-tools"
      }
    }
  ]
}
