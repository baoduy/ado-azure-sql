{
  "manifestVersion": 1,
  "id": "ado-azure-tools",
  "version": "0.1.1",
  "name": "Azure Devops Azure Tools.",
  "description": "The helpfull Azure Tools for Azure Devops.",
  "publisher": "Duy",
  "public": true,
  "icons": {
    "default": "static/images/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": ["Azure Pipelines"],
  "categories": ["Azure Pipelines"],
  "content": {
    "license": {
      "path": "LICENSE"
    },
    "details": {
      "path": "README.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/baoduy/ado-azure-tools"
  },
  "files": [{ "path": "azure-sql" }, { "path": "azure-streamjob-control" }],
  "contributions": [
    {
      "id": "ado-azure-sql",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "azure-sql"
      }
    },
    {
      "id": "ado-azure-streamjob-control",
      "targets": ["ms.vss-distributed-task.tasks"],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "azure-streamjob-control"
      }
    }
  ]
}
