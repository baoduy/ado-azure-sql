{
  "name": "ado-azure-tool",
  "private": true,
  "version": "99.99.99",
  "scripts": {
    "package": "npm run install-sub && npm run compile && npm run clean && tfx extension create --manifest-globs --rev-version",
    "publish": "tfx extension publish --manifest-globs --no-wait-validation --token $TOKEN",
    "publish-local": "tfx extension publish --manifest-globs --no-wait-validation",
    "clean": "rimraf ./*.vsix",
    "compile": "tsc -p .",
    "install-sub": "node tools/install-all.js",
    "update-sub": "node tools/update-all.js",
    "test": "npm run compile && mocha tests/_suite.js",
    "update": "ncu -u && npm install"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "azure-pipelines-task-lib": "^2.12.1",
    "minimist": ">=1.2.5",
    "mocha": "^8.2.1",
    "sync-request": "^6.1.0",
    "tfx-cli": "^0.8.3",
    "typescript": "4.1.3"
  },
  "dependencies": {}
}
