{
  "name": "ado-azure-tool",
  "private": true,
  "version": "99.99.99",
  "scripts": {
    "package": "npm run clean  && npm run compile  && tfx extension create --rev-version",
    "publish": "tfx extension publish --no-wait-validation --token $TOKEN",
    "publish-local": "tfx extension publish --no-wait-validation",
    "clean": "rimraf ./*.vsix",
    "compile": "tsc -p .",
    "install-sub": "node tools/install-all.js",
    "update-sub": "node tools/update-all.js",
    "test": "npm run compile && mocha tests/_suite.js",
    "update": "ncu -u && npm install"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.2",
    "@types/node": "^14.0.27",
    "azure-pipelines-task-lib": "^2.10.0",
    "minimist": ">=1.2.5",
    "mocha": "^8.1.1",
    "sync-request": "^6.1.0",
    "tfx-cli": "^0.8.1",
    "typescript": "3.9.7"
  },
  "dependencies": {}
}
